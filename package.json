{
	"name": "chunkyserver",
	"version": "0.0.11",
	"description": "A comic-server for ChunkyTNG",
	"author": {
		"email": "mike.ferenduros@gmail.com",
		"name": "Michael Ferenduros"
	},
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "net.mike-ferenduros.chunkyserver",
		"productName": "ChunkyServer",
		"mac": {
			"extraResources": [
				"unrar_MacOSX_10.13.2_64bit"
			],
			"target": "zip",
			"publish": [
				"github"
			]
		},
		"win": {
			"extraResources": [
				"UnRAR.exe"
			],
			"target": "nsis",
			"certificateFile": "authenticode.p12",
			"certificatePassword": "a",
			"publish": [
				"github"
			]
		},
		"nsis": {}
	},
	"devDependencies": {
		"electron": "^2.0.2",
		"electron-builder": "^20.15.1"
	},
	"dependencies": {
		"electron-updater": "^2.21.10",
		"express": "^4.16.3",
		"jquery": "^3.3.1",
		"nat-upnp": "^1.1.1",
		"node-forge": "^0.7.5",
		"pmp": "^0.5.0",
		"qrcode": "^1.2.0",
		"sha1": "^1.1.1",
		"string-natural-compare": "^2.0.2",
		"tmp": "0.0.33",
		"xml-writer": "^1.7.0",
		"yauzl": "^2.10.0"
	}
}
